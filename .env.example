# ProofVault Environment Variables
# Copy this file to .env.local for local development

# WalletConnect Project ID (get one at https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Contract Addresses (after deployment)
VITE_PROOF_REGISTRY_ADDRESS=0x0000000000000000000000000000000000000000
VITE_PROOF_NFT_ADDRESS=0x0000000000000000000000000000000000000000

# Network (polygonAmoy or polygon)
VITE_NETWORK=polygonAmoy
